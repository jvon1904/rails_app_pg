<h1>Update page!</h1>

    <%= form_with(model: @page) do |form| %>
        <%= form.label :title, "Title" %>
        <%= form.text_field :title %>
        <%= form.label :body, "Body" %>
        <%= form.text_field :body %>
        <%= form.submit 'Do it!' %>

    <% end %>

    <%= link_to "Back", @page %>